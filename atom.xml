  <entry>
    <title>Linux延长续航设置</title>
    <link href="http://example.com/2023/11/05/Linux%E5%BB%B6%E9%95%BF%E7%BB%AD%E8%88%AA%E8%AE%BE%E7%BD%AE/"/>
    <id>http://example.com/2023/11/05/Linux%E5%BB%B6%E9%95%BF%E7%BB%AD%E8%88%AA%E8%AE%BE%E7%BD%AE/</id>
